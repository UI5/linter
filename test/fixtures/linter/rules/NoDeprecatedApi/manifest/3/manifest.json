{
	"_version": "2.0.0",

	"sap.app": {
		"id": "com.ui5.manifest.v2.app",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"v4": {
				"uri": "/api/odata-4/",
				"type": "OData",
				"settings": {
					"odataVersion": "4.0"
				}
			}
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "com.ui5.manifest.v2.app.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},

		"dependencies": {
			"minUI5Version": "1.136.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {}
			}
		},

		"handleValidation": true,

		"contentDensities": {
			"compact": true,
			"cozy": true
		},

		"resources": {
			"js": []
		},

		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.ui5.manifest.v2.app.i18n.i18n"
				}
			},
			"odata-v4": {
				"type": "sap.ui.model.odata.v4.ODataModel",
				"settings": {
					"serviceUrl": "/api/odata-v4"
				}
			}
		},

		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "com.ui5.manifest.v2.app.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "main",
					"target": "main"
				}
			],
			"targets": {
				"main": {
					"name": "Main",
					"type": "View"
				},
				"moduleTarget": {
					"name": "module:myapp/views/ModuleView",
					"viewType": "XML"
				}
			}
		}
	}
}
